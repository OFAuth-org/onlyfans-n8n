{
  "name": "n8n-nodes-ofauth",
  "version": "2.0.0",
  "description": "n8n community node for OFAuth API",
  "keywords": [
    "n8n-community-node-package",
    "ofauth"
  ],
  "main": "index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint nodes credentials --ext .ts"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/*.credentials.js"
    ],
    "nodes": [
      "dist/nodes/**/*.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "license": "Apache-2.0"
}